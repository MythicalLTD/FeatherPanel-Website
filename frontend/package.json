{
    "name": "featherpanel-frontend",
    "version": "0.0.1",
    "private": false,
    "publisher": "MythicalSystems",
    "description": "Frontend for FeatherPanel - The next generation server management panel.",
    "author": {
        "name": "NaysKutzu",
        "email": "ghermancassian2008@gmail.com",
        "url": "https://nayskutzu.xyz"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/mythicalltd/featherpanel"
    },
    "bugs": {
        "url": "https://github.com/mythicalltd/featherpanel/issues"
    },
    "homepage": "https://www.mythical.systems",
    "license": "MIT",
    "type": "module",
    "keywords": [
        "featherpanel",
        "vue",
        "vite",
        "server",
        "dashboard",
        "panel"
    ],
    "funding": {
        "type": "github",
        "url": "https://github.com/sponsors/nayskutzu"
    },
    "engines": {
        "node": ">=22.0.0"
    },
    "scripts": {
        "dev": "vite --host",
        "start": "vite preview --host --mode production",
        "build": "NODE_OPTIONS=--max_old_space_size=8096 run-p type-check \"build-only {@}\" --",
        "build-win": "run-p type-check \"build-only {@}\" --",
        "build-debug": "vite build --mode development --no-minify",
        "preview": "vite preview",
        "build-only": "vite build",
        "type-check": "vue-tsc --build --force",
        "lint": "run-p format lint-fix",
        "lint-fix": "eslint . --fix",
        "format": "prettier --log-level silent --write src/",
        "test": "echo \"No tests yet\" && exit 0",
        "clean": "rm -rf dist node_modules",
        "check": "pnpm lint && pnpm type-check",
        "ui:add": "npx shadcn-vue@latest add select"
    },
    "dependencies": {
        "@eslint/js": "^9.37.0",
        "@tailwindcss/vite": "^4.1.14",
        "@tanstack/vue-table": "^8.21.3",
        "@tsconfig/node22": "^22.0.2",
        "@types/node": "^24.7.2",
        "@vitejs/plugin-vue": "^6.0.1",
        "@vue/tsconfig": "^0.8.1",
        "@vueuse/core": "^13.9.0",
        "axios": "^1.12.2",
        "class-variance-authority": "^0.7.1",
        "clsx": "^2.1.1",
        "eslint": "^9.37.0",
        "eslint-config-prettier": "^10.1.8",
        "lucide-vue-next": "^0.545.0",
        "npm-run-all2": "^8.0.4",
        "prettier": "^3.6.2",
        "reka-ui": "^2.5.1",
        "tailwind-merge": "^3.3.1",
        "tailwindcss": "^4.1.14",
        "tw-animate-css": "^1.4.0",
        "typescript": "~5.9.3",
        "typescript-eslint": "^8.46.1",
        "vaul-vue": "^0.4.1",
        "vee-validate": "^4.15.1",
        "vite": "7.1.9",
        "vite-plugin-vue-devtools": "^8.0.2",
        "vue": "^3.5.22",
        "vue-router": "^4.5.1",
        "vue-toastification": "2.0.0-rc.5",
        "vue-tsc": "^3.1.1"
    },
    "devDependencies": {
        "eslint-plugin-license-header": "^0.8.0",
        "eslint-plugin-vue": "^10.5.0",
        "globals": "^16.4.0",
        "vue-eslint-parser": "^10.2.0"
    }
}
